{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "crons": [
      {
        "path": "/conv/anniversaire",
        "schedule": "0 4 * * *"
      },
      {
        "path": "/tiers/updatePorteurProjetAn",
        "schedule": "0 4 1 1 *"
      }
    ]
  }